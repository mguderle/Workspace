<?xml version="1.0" encoding="utf-8"?>
<xs:schema targetNamespace="http://tempuri.org/XMLSchema.xsd"
    elementFormDefault="qualified"
    xmlns="http://tempuri.org/XMLSchema.xsd"
    xmlns:mstns="http://tempuri.org/XMLSchema.xsd"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
>
  <xs:element name="Metadata">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="DatasetLevelMetadata">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Dataset">
                <xs:complexType>
                  <xs:sequence>

                    <xs:element name="Title" type="xs:string" minOccurs="1">
                      <xs:annotation>
                        <xs:documentation>Title of the dataset to be uploaded to the ATTO data portal.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="Abstract" type="xs:string" minOccurs="1">
                      <xs:annotation>
                        <xs:documentation>A brief overview of the resource to be uploaded to the ATTO data portal. The abstract should include basic information that summarizes the dataset. Example: Dataset provides information about water temperature collected in Lake Candia.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="DataOfPhdThesis" type="xs:boolean" minOccurs="1">
                      <xs:annotation>
                        <xs:documentation>Was the dataset collected as part of a doctoral thesis? If so, data users are obliged to consider the special regulations in the data policy regarding PhD data.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="Keywords" type="xs:string" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>Provide a set of related keywords describing the content of the dataset. Please separate the keywords with commas (e.g. flux, CO2, tower).</xs:documentation>
                      </xs:annotation>
                    </xs:element>        
                       
                    <xs:element name="IdentifierType" type="relatedIdentifierType" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>Applies only for data sets, which will be published via MPDL. A persistent identifier that identifies a dataset, assigned by the MPDL for this dataset. Currently, only DOI is possible.</xs:documentation>
                       </xs:annotation>
                    
                       </xs:element>
                    
                     <xs:element name="IdentifierValue" type="xs:string" minOccurs="0">
                       <xs:annotation>
                         <xs:documentation>Applies only for data sets, which will be published via MPDL. Please insert the DOI assigned by the MPDL. e.g. 10.17871/atto_xxxx</xs:documentation>
                       </xs:annotation>
                     </xs:element>                                  
                      
                    <xs:element name ="DataCreators">
                      <xs:annotation>
                        <xs:documentation>The main researchers involved working on the data, or the authors of the publication in priority order. May be a corporate/institutional or personal name.</xs:documentation>
                        <xs:documentation> Name format: Given Family</xs:documentation>
                      </xs:annotation>
                      
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="DataCreator"                                
                                      type="contact"
                                      minOccurs="1"
                                      maxOccurs="unbounded"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                                   
                    <xs:element name ="DataOwners">
                      <xs:annotation>
                        <xs:documentation>Person(s) who own this dataset.</xs:documentation>
                        <xs:documentation> Name format: Given Family</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>

                          <xs:element name="DataOwner"
                                      type="contact"
                                      minOccurs="1"
                                      maxOccurs="unbounded"/>

                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>

                    <xs:element name ="DataContributors">
                      <xs:annotation>
                        <xs:documentation>Person(s) contributed to the data or/and helped to collect the data.</xs:documentation>
                        <xs:documentation> Name format: Given Family</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>

                          <xs:element name="DataContributor"
                                      type="contact"
                                      minOccurs="1"
                                      maxOccurs="unbounded"/>

                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                     
                   <xs:element name="Publisher" type="Publisher" fixed="Max Planck Institute for Biogeochemistry, Jena" minOccurs="0">
                     <xs:annotation>
                       <xs:documentation>Applies only for datasets, which will be published via MPDL. The name of the entity that holds, archives, publishes the dataset. In the case of datasets, "publish" is understood to mean making the data available to the community of researchers.</xs:documentation>
                     </xs:annotation>
                   </xs:element>

                    <xs:element name="Contributor" type="Contributor" fixed="INPA" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>Applies only for datasets, which will be published via MPDL. The institution responsible for collecting, managing, distributing, publishing or otherwise contributing to the development of the resource.</xs:documentation>
                      </xs:annotation>
                    </xs:element>

                    <xs:element name="ContributorType" type="ContributorType" fixed="Sponsor" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>Applies only for datasets, which will be published via MPDL. Person or organisation that issued a contract or under the auspices of which a work has been written, printed, published, developed, etc.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    
                    <xs:element name="ContributorName" type="ContributorName" fixed="Instituto Nacional de Pesquisas da Amazônia, Manaus" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>Applies only for datasets, which will be published via MPDL. The institution responsible for collecting, managing, distributing, publishing or otherwise contributing to the development of the resource.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    
                  <xs:element name="PublicationYear" type="yearType" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Applies only for datasets, which will be published via MPDL. Year when the data is made publicly available. If an embargo period has been in effect, use the date when the embargo period ends.</xs:documentation>
                      <xs:documentation> YYYY</xs:documentation>
                    </xs:annotation>
                  </xs:element>

                  <xs:element name="DataClassification" type="dataClass" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>Principal classification of the data to be uploaded to the ATTO data portal.</xs:documentation>
                      </xs:annotation>
                    </xs:element>

                  <xs:element name="ResourceType" type="resourceType" minOccurs="0">
					          <xs:annotation>
						          <xs:documentation>The type of the dataset to be uploaded to the ATTO data portal.</xs:documentation>
					          </xs:annotation>
				          </xs:element>

                    <xs:element name="Format" type="xs:string" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>Technical format of the original resource.</xs:documentation>
                        <xs:documentation> Use file extension or MIME type where possible. E.g. jpg, png, xlsx, csv, dat ... </xs:documentation>
                      </xs:annotation>
                    </xs:element>
                         
                 <xs:element name="Version" type="xs:string" minOccurs="0">
					          <xs:annotation>
						         <xs:documentation>Version number of the resource/dataset. If the primary resource has changed the version number increases.</xs:documentation>
						         <xs:documentation> Register a new identifier for a major version change. Individual stewards need to determine which are major vs. minor versions.</xs:documentation>
					         </xs:annotation>
				         </xs:element>             
                 
                  </xs:sequence>
                </xs:complexType>
              </xs:element>             
                    
              <xs:element name="Data">
                <xs:complexType>
                  <xs:sequence>

                    <xs:element name="DatasetMethodSpecification" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>Provide a documentation of the procedures followed to produce any object in the dataset to be uploaded to the ATTO data portal. These shall include information about procedure steps, software used within individual steps and source data.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="DatasetSamplingSpecification" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>Allows for a text-based/human readable description of the actual sampling procedures used within the dataset collection. This element shall include information about dataset lineage.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="DatasetSamplingEquipmentInfo" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>Provides information about any instruments used in the data collection or quality control and quality assurance. The description should include vendor, model number, optional equipment, etc.</xs:documentation>
                      </xs:annotation>
                    </xs:element>

                    <xs:element name="DataProcessingLevel"
                                type="xs:string"
                                minOccurs="1">
                      <xs:annotation>
                        <xs:documentation>Processing level of the data to be uploaded to the ATTO data portal. E.g. Level 1B for unit processed data according to the NASA data processing level standards or Level 2A for processed and aggregated data according to the AmeriFlux.</xs:documentation>
                      </xs:annotation>
                    </xs:element>

                    <xs:element name="DataProcessingLevelStandards"
                                type="xs:string"
                                minOccurs="1">
                      <xs:annotation>
                        <xs:documentation>Processing level standards used, e.g. NASA or AmeriFlux and European Fluxes networks.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    
                    <xs:element name="DataCollectionLayer"
                                type="layer"
                                minOccurs="1">
                      <xs:annotation>
                        <xs:documentation>Information if the data were collected above or below ground.</xs:documentation>
                      </xs:annotation>
                    </xs:element>

                    <xs:element name="LayerStart" type="xs:decimal" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>Start height/depth of layer in cm</xs:documentation>
                      </xs:annotation>
                    </xs:element>

                    <xs:element name="LayerEnd" type="xs:decimal" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>End height/depth of layer in cm</xs:documentation>
                      </xs:annotation>
                    </xs:element>

                    <xs:element name="StartDate" type="xs:date">
                      <xs:annotation>
                        <xs:documentation>Start date of the data/time series.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="EndDate" type="xs:date" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>End date of the data/time series.</xs:documentation>
                      </xs:annotation>
                    </xs:element>

                    <xs:element name="TemporalResolution" type="xs:string" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>Information about the temporal resolution of the data/time series e.g. 1 min, monthly.</xs:documentation>
                      </xs:annotation>
                    </xs:element>

                    <xs:element name="NumberOfParameters" type="xs:int" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>Information about the number of parameters in the dataset.</xs:documentation>
                      </xs:annotation>
                    </xs:element>

                    <xs:element name="Location"
                                      type="location"
                                      minOccurs="1">
                      <xs:annotation>
                        <xs:documentation>Information about the location at the ATTO site where measurements were taken. Please use the common name, which is also given in the general map provided by the coordinators.</xs:documentation>
                      </xs:annotation>
                    </xs:element>

                    <xs:element name="LocationOther" type="xs:string" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>In case "Other" is selected in the field "Location", please enter here the name and/or coordinates of the location (e.g. Tall Tower (S 02 08.756 W 059 00.335)).</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    
                    <xs:element name="Abbreviations" minOccurs="1">
                        <xs:annotation>
                        <xs:documentation>Abbreviations used in the dataset to be uploaded to the ATTO data portal (e.g. m for meters, t for tonnes).</xs:documentation>
                        </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>

                          <xs:element name="Abbreviation"
                                      maxOccurs="unbounded">
                            <xs:complexType>
                              <xs:sequence>

                                <xs:element name="Abbreviation" type="xs:string">
                                  <xs:annotation>
                                    <xs:documentation>Abbreviations used in the dataset to be uploaded to the ATTO data portal (e.g. m, t).</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="Description" type="xs:string" minOccurs="0">
                                  <xs:annotation>
                                    <xs:documentation>Long-form of the abbreviations used in the dataset to be uploaded to the ATTO data portal (e.g. meters, tonnes).</xs:documentation>
                                  </xs:annotation>
                                </xs:element>

                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>

                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>   

                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="DataQuality" minOccurs="1">
                <xs:annotation>
                  <xs:documentation>Quality assurance - has the  dataset undergone any quality checks?</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>

                    <xs:element name="CheckPerformed" type="xs:boolean">
                      <xs:annotation>
                        <xs:documentation>Quality assurance - has the  dataset undergone any quality checks?</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="Issues" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>Mention the inconsistencies or errors present in the dataset to be uploaded to the ATTO data portal.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="CurationInfo" type="xs:string" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>Information about data curation.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="PlotInfo" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>In case the dataset is based on a plot experiment, please insert additional information here.</xs:documentation>
                      </xs:annotation>
                <xs:complexType>
                  <xs:sequence>

                    <xs:element name="TotalPlots" type="xs:int" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>Total number of plots the dataset is based on.</xs:documentation>
                      </xs:annotation>
                    </xs:element>

                    <xs:element name="Plot"
                                maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>

                          <xs:element name="PlotSize" type="xs:decimal" minOccurs="0">
                            <xs:annotation>
                              <xs:documentation>Plotsize in sqm.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="NumberOfPlots" type="xs:int" minOccurs="0">
                            <xs:annotation>
                              <xs:documentation>Number of plots of particular size on which dataset is based.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="SubPlots"
                                      minOccurs="0"
                                      maxOccurs="unbounded">
                            <xs:annotation>
                             <xs:documentation>In case the experiment contains subplots, please insert additional information here.</xs:documentation>
                           </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>

                                <xs:element name="SubPlotSize" type="xs:decimal" minOccurs="0">
                                  <xs:annotation>
                                    <xs:documentation>Subplotsize in sqm.</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="NumberOfSubplots" type="xs:int" minOccurs="0">
                                  <xs:annotation>
                                    <xs:documentation>Number of subplots of particular size on which dataset is based.</xs:documentation>
                                  </xs:annotation>
                                </xs:element>

                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>

                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                                  
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              
              <xs:element name="Software">
                  <xs:annotation>
                   <xs:documentation>Information on the software used for dataset creation, quality check and assurance.</xs:documentation>
                  </xs:annotation>
                <xs:complexType>
                  <xs:sequence>

                    <xs:element name="Application" 
                                minOccurs="1"
                                maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>

                          <xs:element name="Name" type="xs:string" minOccurs="1">
                            <xs:annotation>
                              <xs:documentation>Name of the software used for dataset creation, quality check and assurance.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="Version" type="xs:string" minOccurs="1">
                            <xs:annotation>
                              <xs:documentation>If applicable, version of the software.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="Availability">
                            <xs:complexType>
                              <xs:sequence>

                                <xs:element name="CompanyName" type="xs:string" minOccurs="0">
                                  <xs:annotation>
                                    <xs:documentation>Name of the Software developer.</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="URL" type="xs:anyURI" minOccurs="0">
                                  <xs:annotation>
                                    <xs:documentation>URL of the Software developer.</xs:documentation>
                                  </xs:annotation>
                                </xs:element>

                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>

                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>

                  </xs:sequence>
                </xs:complexType>
              </xs:element>

              <xs:element name="AdditionalInfo" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Provide information if any additional related datasets where used to create this dataset, if applicable.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    
                    <xs:element name="RelatedDataset" type="xs:string" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>Provide information if any additional related datasets where used to create this dataset, if applicable.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="DatasetLevelAdditionalMetadata"
                                type="xs:string"
                                minOccurs="0"/>

                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="EnvironmentalLevelMetadata" minOccurs="1" >
          <xs:annotation>
            <xs:documentation>Provide information on the environmental and coverage data of the dataset to be uploaded to the ATTO data portal.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="SpatialCoverage" type="spatialcoverage">
                <xs:annotation>
                  <xs:documentation>Provide information on the spatial coverage of the dataset to be uploaded to the ATTO data portal, e.g. Local; Regional; National; Continental; Global.</xs:documentation>
                </xs:annotation>
              </xs:element>

              <xs:element name="GeoreferenceSource"
                          type="xs:string"
                          minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Source used for georeferencing such as gazetter, online resource url and name, gps type etc.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="GeoreferenceRemarks"
                          type="xs:string"
                          minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Remarks related to georeferencing.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="GeoLocations">
                <xs:annotation>
                  <xs:documentation>Provide detailed information about the location at the ATTO site where measurements were taken.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    
                    <xs:element name="GeoLocation"
                                minOccurs="1"
                                maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>
                    
                          <xs:element name="GeoLocationName"
                                      type="xs:string">
                            <xs:annotation>
                              <xs:documentation>Provide detailed information about the location at the ATTO site where measurements were taken.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="Latitude"
                                      type="latitude">
                            <xs:annotation>
                              <xs:documentation>Latitude of the location in decimals.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="Longitude"
                                      type="longitude">
                            <xs:annotation>
                              <xs:documentation>Longitude of the location in decimals.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          
                          <xs:element name="CoordinateReferenceSystem"
                                      type="coordinateReferenceSystem"
                                      default="WGS84"
                                      minOccurs="1">
                            <xs:annotation>
                              <xs:documentation>WGS 84; UTM Zone 38 North ETRS89 etc.</xs:documentation>
                            </xs:annotation>
                          </xs:element>

                          <xs:element name="Precision"
                                     type="xs:string"
                                     minOccurs="0">
                            <xs:annotation>
                              <xs:documentation>How precise is the location Lat/Long reading?</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          
                          <xs:element name="Altitude"
                                      type="altitude"
                                      minOccurs="0">
                            <xs:annotation>
                              <xs:documentation>Altitude of the location in metres.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="Depth"
                                      type="depth"
                                      minOccurs="0">
                            <xs:annotation>
                              <xs:documentation>If aquatic then depth of the location.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                                                   
                          <xs:element name="Slope"
                                      type="slope"
                                      minOccurs="0">
                            <xs:annotation>
                              <xs:documentation>Angle of the slope.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="Aspect"
                                      type="aspect"
                                      minOccurs="0">
                            <xs:annotation>
                              <xs:documentation>Direction of the slope.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="Habitat"
                                      type="xs:string"
                                      minOccurs="0">
                            <xs:annotation>
                              <xs:documentation>Habitat info of the location.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                   
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              
              <xs:element name="Remarks" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Additional environmental metadata that you think is not covered in this schema.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>

                    <xs:element name="EnvironmentalLevelAdditionalMetadata"
                                type="xs:string"
                                minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>Additional environmental metadata that you think is not covered in this schema.</xs:documentation>
                      </xs:annotation>
                    </xs:element>

                  </xs:sequence>
                </xs:complexType>
              </xs:element>

            </xs:sequence>
          </xs:complexType> 
        </xs:element>


        <xs:element name="PublishedIn" minOccurs="1">
          <xs:annotation>
            <xs:documentation>If data are already published in a repository or in the context of a paper, please insert here the respective DOI. e.g. 10.17871/atto_xxxx</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="DOI"  type="xs:string" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>If data are already published in a repository or in the context of a paper, please insert here the respective DOI. e.g. 10.17871/atto_xxxx</xs:documentation>
                </xs:annotation>
              </xs:element>

                  </xs:sequence>
                </xs:complexType>
              </xs:element>


        <xs:element name="Project">
          <xs:annotation>
            <xs:documentation>Name of the project for which the dataset is collected. A project can have many datasets with different dataset titles.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="ProjectTitle" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Name of the project for which the dataset is collected. A project can have many datasets with different dataset titles.</xs:documentation>
                </xs:annotation>
              </xs:element>
              
              <xs:element name="Funders" minOccurs="1">
                        <xs:annotation>
                        <xs:documentation>Information about the funder(s) of the Project e.g. BMBF, INPA.</xs:documentation>
                        </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>

                          <xs:element name="Funder"
                                      maxOccurs="unbounded">
                            <xs:complexType>
                              <xs:sequence>

                                <xs:element name="Funder" type="xs:string">
                                  <xs:annotation>
                                    <xs:documentation>Funder of the Project e.g. BMBF, INPA.</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="FunderIdentifier" type="xs:string" minOccurs="0">
                                  <xs:annotation>
                                    <xs:documentation>Uniquely identifies a funding entity, according to various types.</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="AwardNumber" type="xs:string" minOccurs="0">
                                 <xs:annotation>
                                  <xs:documentation>The code assigned by the funder to a sponsored award (grant).</xs:documentation>
                                 </xs:annotation>
                               </xs:element>
                               <xs:element name="AwardTitle" type="xs:string" minOccurs="0">
                                 <xs:annotation>
                                  <xs:documentation>The human readable title of the award (grant).</xs:documentation>
                                 </xs:annotation>
                               </xs:element>

                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>

                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>   
                
              

            </xs:sequence>
          </xs:complexType>
        </xs:element>
              
      </xs:sequence>
          </xs:complexType>
        </xs:element>
  
  <xs:simpleType name="Publisher">
    <xs:restriction base="xs:string">
